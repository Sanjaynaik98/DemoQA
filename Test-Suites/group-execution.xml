<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DemoQA Test Suite">

	<listeners>
		<listener
			class-name="com.demoqa.webautomation.utilities.ExtentReportManager" />
	</listeners>

	<test name="Element Tests">
		<parameter name="os" value="windows" />
		<parameter name="browser" value="chrome" />
		<groups>
			<run>
				<include name="webtable" />
				
			</run>
		</groups>
		<classes>
			<class
				name="com.demoqa.webautomation.testcases.ElementsFunctionalityTest">
			</class>
		</classes>
	</test>

</suite>
